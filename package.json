{
  "name": "@qualweb/locale",
  "version": "0.1.12",
  "description": "Locales for @qualweb/core evaluation reports.",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "tsc": "tsc",
    "test": "mocha",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "build": "rimraf dist && tsc --build",
    "prepare": "npm run build",
    "release": "npm run build && npx changeset publish",
    "release-snapshot": "npm run build && npx changeset version --snapshot next && npx changeset publish --tag next"
  },
  "homepage": "https://github.com/qualweb/locale#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qualweb/locale.git"
  },
  "bugs": {
    "url": "https://github.com/qualweb/locale/issues"
  },
  "keywords": [
    "qualweb",
    "accessibility",
    "a11y",
    "locale"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "Jo√£o Vicente",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@qualweb/types": "0.7.16",
    "@tsconfig/recommended": "^1.0.1",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "mocha": "^9.1.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0"
  }
}
